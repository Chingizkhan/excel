{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "arrow-parents": "off",
        "linebreak-style": ["error", "windows"],
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "indent": "off",
    },
    "extends": ["eslint:recommended", "google"],
}